pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Development'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Pre-Deploy') {
            steps {
                echo 'Mise en pre-production'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Mise en production'
            }
        }
    }
}
